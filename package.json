{
  "name": "non-commercial",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint src/ --ext .ts,.tsx --fix --cache",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix --cache",
    "lint": "npm run eslint && npm run stylelint"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.29.1",
    "@arco-themes/react-arco-pro": "^0.0.7",
    "@reduxjs/toolkit": "^1.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1"
  },
  "devDependencies": {
    "@arco-plugins/vite-plugin-svgr": "^0.7.2",
    "@arco-plugins/vite-react": "^1.0.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.1",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{css, less}": [
      "npm run stylelint",
      "git add"
    ]
  }
}
